  "scripts": {
    "start": "electron --debug ./dist/main.js",
    "build:main": "tsc",
    "build:render": "webpack",
    "test": "jest",
    "package:mac": "tsc && webpack && electron-builder --mac --x64",
    "package:windows": "tsc && webpack && electron-builder --win --x64",
    "package:linux": "tsc && webpack && electron-builder --linux --x64"
  },
  "build": {
    "productName": "TODO",
    "appId": "todo.exsample.com",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*.js",
      "./dist/index.html",
      "./dist/main.js",
      "./icon/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./icon/todo.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./icon/todo.ico",
      "target": "msi"
    }
  }
